#!/usr/bin/env ruby

$: << File.expand_path('../lib', File.dirname(__FILE__))

require 'drb'
require 'drb/ssl'
require 'rclconf'
require 'yus/session'
require 'yus/helpers'
require 'getoptlong'

opts = Yus.default_opts
filename = ARGV.first

session = Yus.session(opts)
session.dump_to_yaml(filename)

